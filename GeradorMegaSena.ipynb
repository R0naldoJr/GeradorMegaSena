        "from random import sample\n",
        "\n",
        "# Gera 3 vezes 6 números de 1 a 60\n",
        "jogos = 0\n",
        "while jogos < 3:\n",
        "  numeros = sample(range(1,60), 6)\n",
        "  jogos += 1\n",
        "  print(numeros)\n"
      ]
    }
